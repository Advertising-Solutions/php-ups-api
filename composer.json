{
  "name": "gabrielbull/ups-api",
  "type": "library",
  "description": "PHP UPS API",
  "keywords": [
    "ups",
    "api",
    "wrapper",
    "quantum view",
    "tracking",
    "shipping",
    "rate",
    "rating",
    "time in transit",
    "tradeability"
  ],
  "version": "1.2.0",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Bull",
      "email": "me@gabrielbull.com"
    },
    {
      "name": "Sebastien Vergnes",
      "email": "sebastien@vergnes.eu"
    },
    {
      "name": "Michael Williams",
      "email": "michael.williams@limelyte.com"
    },
    {
      "name": "Stefan Doorn",
      "email": "stefan@efectos.nl"
    },
    {
      "name": "Eduard Sukharev",
      "email": "sukharev.eh@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1 || ^8.0",
    "jane-php/open-api-runtime": "~6.1.0",
    "psr/http-client-implementation": "*",
    "php-http/client-common": "^1.9 || ^2.0",
    "php-http/discovery": "^1.7",
    "psr/log": "^1.0",
    "zircote/swagger-php": "^3.1"
  },
  "require-dev": {
    "jane-php/open-api-3": "~6.1.0",
    "symfony/http-client": "^5.1",
    "nyholm/psr7": "^1.2",
    "friendsofphp/php-cs-fixer": "^2.16",
    "symfony/phpunit-bridge": "^4.3",
    "opis/json-schema": "^1.0",
    "symfony/console": "^5.1",
    "symfony/contracts": "^2.1",
    "symfony/process": "^5.1",
    "symfony/string": "^5.1"
  },
  "conflict": {
    "php-http/httplug": "< 2.0"
  },
  "suggest": {
    "ext-soap": "Required if you need the Tradeability API of UPS"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.2-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "Ups\\": "src/",
      "Ups\\Api\\": "generated/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ups\\Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
